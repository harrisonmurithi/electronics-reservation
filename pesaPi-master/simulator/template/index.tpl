<head><link href="App_Themes/Kenya/default0707.css" type="text/css" rel="stylesheet" /><title>
	M-PESA Login
</title><meta http-equiv="Pragma" content="no-cache" /><meta http-equiv="Expires" content="-1" /><meta http-equiv="CACHE-CONTROL" content="NO-CACHE" /></head>
<body>
    <form name="form1" method="post" action="Default.aspx" id="form1">
<input type="hidden" name="__VIEWSTATE" id="__VIEWSTATE" value="{VIEWSTATE}" />

        <div class="header">
            <div class="LogoRight">
                <a id="lnkMPESA" class="floatRight"><img src="/ke/Images/m-pesa.gif" border="0" /></a>
            </div>
              
        </div>
        <div class="Container">
            <div class="sidebar">             
            </div>
            <div class="content" style="left: 0px; top: 0px">
                <h1>Welcome</h1>
                <p></p>
                
                        <h3>Welcome to the M-PESA Administration Website</h3>
                        
                        <table id="LoginCtrl" cellspacing="0" cellpadding="0" border="0" width="638">
	<tr>
		<td>
                                <table class="FormTable">                                           
                                    <tr>
                                        <th style="width: 180px">
                                            <label for="LoginCtrl_UserName" id="LoginCtrl_UserNameLabel">Username</label>:</th>
                                        <td style="width: 409px">
                                            <input name="LoginCtrl$UserName" type="text" id="LoginCtrl_UserName" />
                                            
                                                </td>
                                    </tr>
                                    <tr>
                                        <th style="width: 180px">
                                            <label for="LoginCtrl_Password" id="LoginCtrl_PasswordLabel">Password</label>:</th>
                                        <td style="width: 409px">
                                            <input name="LoginCtrl$Password" type="password" id="LoginCtrl_Password" />
                                            &nbsp;&nbsp;
                                        </td>
                                    </tr>
                                    <tr>
                                        <th style="width: 180px">
                                            <label for="LoginCtrl_txtOrganisationName" id="LoginCtrl_Label1">Organisation</label>:</th>
                                        <td style="width: 409px">
                                            <input name="LoginCtrl$txtOrganisationName" type="text" value="M-PESA" id="LoginCtrl_txtOrganisationName" />
                                            &nbsp;
                                            &nbsp;</td>
                                    </tr>
                                    <tr>
                                        <th style="width: 180px">&nbsp;</th>
                                        <td style="width: 409px">
                                            <input id="LoginCtrl_chkUseAjax" type="checkbox" name="LoginCtrl$chkUseAjax" checked="checked" /><label for="LoginCtrl_chkUseAjax">Use Advanced Interface</label>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th style="width: 180px">&nbsp;</th>
                                        <td style="color: red; width: 409px;" colspan="2" align="center">                                                
                                            
                                        </td>
                                    </tr>
                                    <tr>
                                        <th style="width: 180px">&nbsp;</th>
                                        <td style="width: 409px">
                                            <input type="submit" name="LoginCtrl$LoginButton" value="Log In" onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;LoginCtrl$LoginButton&quot;, &quot;&quot;, true, &quot;Login1&quot;, &quot;&quot;, false, false))" id="LoginCtrl_LoginButton" class="button" />
                                            <input type="submit" name="LoginCtrl$btnForgotPassword" value="Forgot Password" id="LoginCtrl_btnForgotPassword" disabled="disabled" class="button" /></td>
                                    </tr>                                        
                                </table>
                                <br />       
                            </td>
	</tr>
</table>
                        
                        <table class="FormTable">
                            <tr>
                                <td colspan="2">
